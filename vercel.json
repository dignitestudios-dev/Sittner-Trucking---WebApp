{
    "rewrites":  [
      {"source": "/(.*)", "destination": "/"}
    ],
    "headers": [
      {
        "source": "/firebase-messaging-sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }